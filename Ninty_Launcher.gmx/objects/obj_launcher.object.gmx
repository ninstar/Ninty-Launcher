    if(thumb == ""){

        //SFX
        audio_play_sfx(snd_addgame_step1);
        
        //Buscar miniatura...
        thumb = get_open_filename("Thumbnail (256x256)|*.png;*.jpg","");
        if(thumb != ""){
        
            //Miniatura com anima√ß√£o
